<?xml version="1.0" encoding="UTF-8"?>
<diagram xmlns="http://www.entitymodelling.org/diagram"
         xmlns:diagram="http://www.entitymodelling.org/diagram"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">

	  <text_style>
       <id>title</id>
       <fill>black</fill>
       <font-size>20</font-size>
       <font-family>Arial</font-family>
       <font-weight>
         <bold/>
      </font-weight>
       <font-style>
         <normal/>
      </font-style>
       <text-decoration>
         <underline/>
      </text-decoration>
    </text_style>
   <text_style>
       <id>normal</id>
       <fill>black</fill>
       <font-size>11</font-size>
       <font-family>Arial</font-family>
       <font-weight>
         <normal/>
      </font-weight>
       <font-style>
         <normal/>
      </font-style>
    </text_style>
   <text_style>
       <id>italic</id>
       <fill>black</fill>
       <font-size>11</font-size>
       <font-family>Arial</font-family>
       <font-weight>
         <normal/>
      </font-weight>
       <font-style>
         <italic/>
      </font-style>
    </text_style>
   <text_style>
       <id>bold</id>
       <fill>black</fill>
       <font-size>11</font-size>
       <font-family>Arial</font-family>
       <font-weight>
         <bold/>
      </font-weight>
       <font-style>
         <normal/>
      </font-style>
    </text_style>
	  <shape_style>
         <id>margin</id>
         <stroke>black</stroke>
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray>
         <fill>#FF44FF</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule>evenodd</fill-rule>
      </shape_style>
   <shape_style>
         <id>padding</id>
         <stroke>black</stroke> 
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray>
         <fill>#FFFF00</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule> evenodd</fill-rule>
      </shape_style>
   <shape_style>
         <id>outer</id>
         <stroke>black</stroke> 
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray> 
         <fill>#0066FF</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule>evenodd</fill-rule>
      </shape_style>
   <shape_style>
          <id>outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>outlinedebug</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>0.04</stroke-width>
      </shape_style>
   <shape_style>
          <id>route</id>
          <fill-opacity>0</fill-opacity>
          <stroke>blue</stroke>
          <stroke-width>0.04</stroke-width>
      </shape_style>
   <shape_style>
          <id>eteven</id>
          <fill>#FFFFCC</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>etodd</id>
          <fill>white</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
         <id>point</id>
         <stroke>red</stroke> 
         <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>entity_type_outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>red</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>group_outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>blue</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>tickmarks</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>offwhite</id>
          <fill>#FCFCFC</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
   <shape_style>
          <id>gradient</id>
          <fill>url(#topdowngrey)</fill>
      </shape_style>
   <shape_style>
          <id>grey</id>
          <fill>grey</fill>
      </shape_style>
   <shape_style>
          <id>invisible</id>
          <fill-opacity>0</fill-opacity>
          <stroke>none</stroke>
      </shape_style>

	  <default>
		    <hmin>1.0</hmin>
		    <wmin>1.0</wmin>
		    <margin>0.3</margin>
		    <padding>0</padding>
		    <packing>horizontal</packing>
		    <text_style>normal</text_style>
		    <shape_style>outline</shape_style>
		    <debug-whitespace>true</debug-whitespace>
	  </default>
	  <h>10</h>
	  <margin>0.5</margin>
	  <label>
      <text>xabsolute.xml</text>
   </label>
	  <enclosure>
		    <id>A</id>
		    <x>
         <abs>0.1</abs>
      </x>
		    <y>
         <abs>2</abs>
      </y>
		    <h>2.0</h>
		    <enclosure>
			      <id>A1</id>
			      <x>
            <abs>0.6</abs>
         </x>
			      <y>
            <abs>2.4</abs>
         </y>
			      <h>1</h>        
		    </enclosure>
		    <enclosure>
			      <id>A2</id>
			      <x>
            <abs>2.8</abs>
         </x>
			      <y>
            <abs>2.4</abs>
         </y>
			      <h>1</h>         
		    </enclosure>
		    <enclosure>
			      <id>A3</id>
			      <x>
            <abs>5.0</abs>
         </x>
			      <y>
            <abs>2.4</abs>
         </y>
			      <h>1</h>
		    </enclosure>
	  </enclosure>
</diagram>
